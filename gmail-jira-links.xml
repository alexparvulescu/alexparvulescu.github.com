<?xml version="1.0" encoding="UTF-8"?>
<Module>
  <ModulePrefs title="Apache Jira Gmail Link Generator"
    description="Extracts Apache Jira issue references and turns them into links."
    height="20"
    author="Alex Parvulescu"
    author_email="alex@pfalabs.com">
    <Require feature="dynamic-height"/>
    <Require feature="google.contentmatch">
      <Param name="extractors">
        google.com:EmailBodyExtractor
      </Param>
    </Require>
  </ModulePrefs>
  <Content type="html" view="card">
<![CDATA[
<center>
<table id=thetab cellpadding=0 cellspacing=6>
<tr><th>Jcr Links Found</th></tr>
<tr><td>-none-</td></tr>
</table>
</center>
]]>
  </Content>
</Module>

